            <View
                android:layout_width="100dp"
                android:layout_height="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="5dp"
                android:background="@color/colorPrimaryLight" />

        <ScrollView
            android:id="@+id/history_scroll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="visible">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="match_parent"
                    android:layout_height="1258dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/ek_history_2016" />

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="match_parent"
                    android:layout_height="2700dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/ek_history_2015" />

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="match_parent"
                    android:layout_height="2800dp"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/ek_history_2014" />
            </LinearLayout>
        </ScrollView>
